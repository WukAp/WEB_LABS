    <body>
        <div class ="scrollable">
            - for(var key in usersNews)
                header
                if usersNews[key].avatar
                    img(src=usersNews[key].avatar, height="50")
                else           
                    img(src="https://sun9-72.userapi.com/impg/aBoVpnhiytlSDTi5Qr0ZRxsve9UbjwHppcky0w/3zwhC2ru5hc.jpg?size=828x823&quality=96&sign=8de36720217172819e26d7014516fee4&type=album", height="50")
                a(href=`/adminModule/users/${usersNews[key].id}`)=usersNews[key].name
                div(class = "padding_03")
                    p=usersNews[key].body
        </div>
        <script type="text/javascript" src="/js/script.js"></script>
    </body>
<html>